<h1> Join Now </h1>

<%= form_for (@client) do |i| %>

    <%= f.label :first_name %>
    <%= f.text_field :first_name %>

    <%= f.label :last_name %>
    <%= f.text_field :last_name %>

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :phone %>
    <%= f.phone_field :phone %>

    <%= f.label :password%>
    <%= f.password_field :password %>

    <%= f.submit "Sign In" %>
    

    <% end %>